{
  "name": "BlogFront",
  "version": "0.0.1",
  "description": "My GitHub blog front",
  "main": "",
  "scripts": {
    "browser-sync": "browser-sync --files='dist/*.css' --files='dist/*.html' --no-open ./dist",
    "webpack-dev-server": "TS_NODE_FILES=true TS_NODE_PROJECT=./configs/tsconfig.node.json webpack-dev-server -r ts-node/register -r esm --hot --config ./configs/webpack.config.ts",
    "webpack": "TS_NODE_FILES=true TS_NODE_PROJECT=./configs/tsconfig.node.json yarn ts-node `yarn bin`/webpack -r esm --config ./configs/webpack.config.ts",
    "s": "TS_NODE_FILES=true TS_NODE_PROJECT=./configs/tsconfig.node.json start",
    "start": "TMUX= tmux -f '' -S /tmp/BlogFront-tmux.sock new-session \\; split-window -h \\; select-pane -t %0 \\; send-keys C-m \\; send-keys 'yarn webpack-serve' C-m \\; select-pane -t %1 \\; send-keys 'yarn shadow-cljs server -A:nrepl' C-m"
  },
  "githubBlogRepo": "bolasblack/bolasblack.github.com",
  "keywords": [],
  "author": "c4605 <bolasblack@gmail.com>",
  "license": "MIT",
  "start": {
    "file": "./configs/start_tasks.ts",
    "require": [
      "ts-node/register"
    ],
    "reporter": "@start/reporter-verbose"
  },
  "dependencies": {
    "airtable": "^0.5.2",
    "aws-sdk": "^2.113.0",
    "create-react-class": "^15.6.3",
    "lodash.identity": "^3.0.0",
    "lodash.isobjectlike": "^4.0.0",
    "lodash.kebabcase": "^4.0.0",
    "lodash.lowerfirst": "^4.0.0",
    "lodash.startcase": "^4.0.0",
    "lodash.upperfirst": "^4.3.1",
    "octokat": "^0.10.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dom-server": "^0.0.5"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/del": "^3.0.1",
    "@types/globby": "^8.0.0",
    "@types/gulp": "^4.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/make-dir": "^1.0.3",
    "@types/node": "^10.5.2",
    "@types/node-sass": "^3.10.32",
    "@types/webpack": "^4.4.6",
    "@types/webpack-dev-server": "^3.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "esm": "^3.0.72",
    "globby": "^8.0.1",
    "gulp-plumber": "^1.1.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "sass": "^1.13.4",
    "sass-loader": "^7.1.0",
    "shadow-cljs": "^2.6.8",
    "style-loader": "^0.23.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
